{
  "expo": {
    "name": "Snapfood",
    "slug": "snapfood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "foodsnap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.masiru.snapfood",
      "infoPlist": {
        "NSCameraUsageDescription": "FoodSnap needs camera access to take photos of your food for nutrition analysis.",
        "NSPhotoLibraryUsageDescription": "FoodSnap needs photo library access to select food photos for analysis."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF6B6B"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.masiru.snapfood",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "com.android.vending.BILLING"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#FF6B6B"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "FoodSnap needs camera access to take photos of your food."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "FoodSnap needs access to your photos to analyze food images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8c8768a4-10f3-4846-894a-25b99f25db6d"
      }
    }
  }
}
